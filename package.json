{
  "name": "sebabiblio",
  "version": "1.0.0",
  "description": "Sistema Integral de Biblioteca Escolar",
  "scripts": {
    "start": "electron .",
    "pack-win": "electron-packager . SebaBiblio --platform=win32 --arch=x64 --out=dist --overwrite",
    "pack-linux": "electron-packager . SebaBiblio --platform=linux --arch=x64 --out=dist --overwrite",
    "pack-mac": "electron-packager . SebaBiblio --platform=darwin --arch=x64 --out=dist --overwrite",
    "pack-installer": "electron-builder --win --x64"
  },
  "author": "Sebastián Nelson Jesús Bertello",
  "license": "MIT",
  "main": "main.js",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.6.0",
    "electron-packager": "^17.1.1"
  },
  "dependencies": {
    "jsbarcode": "^3.12.1",
    "jspdf": "^3.0.3"
  }
}
